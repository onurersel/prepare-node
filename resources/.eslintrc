{
	"parser": "babel-eslint",
	"env": {
		"es6": true,
		"browser": false
	},
	"globals": {
		"fetch": false,
		"browser": false
	},
	"extends": ["airbnb"],
	"parserOptions": {
		"ecmaFeatures": {
			"experimentalObjectRestSpread": true
		},
		"sourceType": "module"
	},
	"rules": {
		"func-names": [0],
		"no-undef": [0],
		"no-console": [0],
		"no-param-reassign": [0],
		"prefer-arrow-callback": [0],
		"class-methods-use-this": [0],
		"import/no-dynamic-require": [0],
		"no-await-in-loop": [0],
		"guard-for-in": [0],
		"global-require": [0],
		"import/prefer-default-export": [0],
		"import/no-extraneous-dependencies": ["error", {"devDependencies": true}]
	},
}
